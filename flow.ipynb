    "# -*- coding: UTF-8 -*-\n",
    "\n",
    "# 引入 TensorFlow 库\n",
    "import tensorflow as tf\n",
    "\n",
    "# 设置了gpu加速提示信息太多了，设置日志等级屏蔽一些\n",
    "import os\n",
    "os.environ['TF_CPP_MIN_LOG_LEVEL']='3'\n",
    "\n",
    "a = tf.constant(2)\n",
    "b = tf.constant(3)\n",
    "c = tf.multiply(a,b)\n",
    "d = tf.add(c, 1)\n",
    "\n",
    "with tf.Session() as sess:\n",
    "    print (sess.run(d))"
